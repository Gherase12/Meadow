// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Partner {
  id      Int    @id @default(autoincrement())
  name    String
  image   String
  votes   Int    @default(value: 0)
  twitter String
  website String
  discord String
  voters User[]

  
}

model User {
  id       Int             @id @default(autoincrement())
  address  String
  projectsVoted Partner[]
}


